{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react", "react-hooks", "prettier"],
    "rules": {
      "prettier/prettier": "error",
      "react/prop-types": "off",
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  }
  